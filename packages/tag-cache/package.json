	"description": "A simple memory cache with invalidation by tags",
	"author": "Maksim Peskov <kostarsf@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/KostarSf/silver-utils.git",
		"directory": "packages/tag-cache"
	},
	"homepage": "https://github.com/KostarSf/silver-utils/tree/main/packages/tag-cache#readme",
